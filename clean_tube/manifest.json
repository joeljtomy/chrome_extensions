{
  "manifest_version": 3,
  "name": "Clean up YouTube",
  "description": "Clean up youtube from distractions",
  "version": "1",
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["inject.js"],
      "css": ["inject.css"]
    }
  ],
  "icons": {
    "16": "icon-16x16.png",
    "32": "icon-32x32.png",
    "48": "icon-48x48.png",
    "128": "icon-128x128.png"
  }
}
