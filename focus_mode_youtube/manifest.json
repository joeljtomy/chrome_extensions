{
  "manifest_version": 3,
  "name": "Focus Tube",
  "description": "Focus mode for youtube",
  "version": "1",
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["inject.js"],
      "css": ["inject.css"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
